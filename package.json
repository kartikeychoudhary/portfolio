{
  "name": "kartikey-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mock": "concurrently \"npm run mock:api\" \"ng serve --proxy-config proxy.conf.json\"",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "test:coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "mock:api": "json-server db.json --port 3000",
    "verify": "npm run lint && npm run test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.1.3",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@primeuix/themes": "^2.0.3",
    "ag-grid-angular": "^35.0.1",
    "ag-grid-community": "^35.0.1",
    "chart.js": "^4.5.1",
    "ng2-charts": "^8.0.0",
    "ngx-image-cropper": "^9.1.6",
    "ngx-quill": "^30.0.1",
    "primeicons": "^7.0.0",
    "primeng": "^21.1.1",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "angular-eslint": "21.2.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "jsdom": "^27.1.0",
    "json-server": "^1.0.0-beta.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.50.1",
    "vitest": "^4.0.8"
  }
}
